<shapes name="mxGraph.basic2">
    <shape name="直线" h="5" w="90" aspect="variable" strokewidth="inherit">
        <foreground>
            <path>
                <move x="0" y="2.5" />
                <line x="90" y="2.5" />
            </path>
            <stroke />
        </foreground>
    </shape>
    <shape name="矩形" h="30" w="90" aspect="variable" strokewidth="inherit">
