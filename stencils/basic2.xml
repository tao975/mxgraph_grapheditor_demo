            </path>
            <stroke/>
            <path>
                <move x="0" y="30" />
                <line x="90" y="30" />
            </path>
            <stroke />
            <path>
                <move x="0" y="0" />
                <line x="0" y="30" />
            </path>
            <stroke />
            <path>
                <move x="90" y="30" />
                <line x="90" y="0" />
            </path>
            <stroke />
        </foreground>
    </shape>
